<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.temicommunication">
    <uses-permission android:name="android.permission.CAMERA" /> //lee
    <uses-permission android:name="android.permission.RECORD_AUDIO" /> //lee
    <uses-permission android:name="android.permission.INTERNET" /> //lee


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="CareService"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TemiCommunication.NoActionBar">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:exported="true">
        <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity" android:exported="false">
        </activity>
        <activity android:name=".GuardianActivity" android:exported="false">
        </activity>
        <activity android:name=".EmergencyCancelActivity" android:exported="false">
        </activity>
        <meta-data
            android:name="com.robotemi.sdk.metadata.SKILL"
            android:value="CareService"/>
    </application>
</manifest>
